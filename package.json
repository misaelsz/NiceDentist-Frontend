{
  "name": "nicedentist-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "docker:build": "docker build -t nicedentist-frontend .",
    "docker:build-dev": "docker build -f Dockerfile.dev -t nicedentist-frontend-dev .",
    "docker:run": "docker run -p 3000:80 nicedentist-frontend",
    "docker:run-dev": "docker run -p 3001:3000 -v \"$(pwd):/app\" -v /app/node_modules nicedentist-frontend-dev",
    "docker:up": "docker-compose up -d",
    "docker:up-dev": "docker-compose --profile dev up -d",
    "docker:down": "docker-compose down",
    "docker:up-standalone": "docker-compose -f docker-compose.standalone.yml up -d",
    "docker:up-standalone-dev": "docker-compose -f docker-compose.standalone.yml --profile dev up -d",
    "docker:down-standalone": "docker-compose -f docker-compose.standalone.yml down"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.26.0",
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
